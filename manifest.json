{
  "manifest_version": 2,

  "name": "Key to Google",
  "description": "Adds keyboard shortcut functionality to google search results pages",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["http://www.google.com/*", "https://www.google.co.uk/*"],
      "js": ["key_to_google.js"]
    }
  ],

  "permissions": [
    "https://www.google.com/",
    "https://www.google.co.uk/"
  ]
}
