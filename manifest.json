{
  "manifest_version": 2,

  "name": "Key to Google",
  "description": "Adds keyboard shortcut functionality to google search results pages",
  "version": "1.1",

  "content_scripts": [
    {
      "matches": ["http://www.google.com/*", "https://www.google.co.uk/*"],
      "js": ["key_to_google.js"]
    }
  ],

  "permissions": [
    "https://www.google.com/",
    "https://www.google.co.uk/"
  ],
   
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "128": "icon128.png" }
}
